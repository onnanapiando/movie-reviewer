<h1>Editing Review</h1>



<%= form_for([@movie, @review], :html => { :multipart => true }) do |f| %>


  <div class="field">
     <%#= <div class="star-rating" data-number= <%= @review.rating %>
    <div class="star-rating"></div>
  </div>
  <div class="field">
    <%= f.label :comment %><br>
    <%= f.text_area :comment %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>



<script>
 $('.star-rating').raty( {

  path: '/assets/',
  scoreName: 'review[rating]'
 })

</script>

<%= link_to 'Show', @movie %> |
<%= link_to 'Back', movies_path %>