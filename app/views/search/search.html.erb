<h1>Movies Search</h1>
 
<%= form_for search_path, method: :get do |f| %>
  <p>
    <%= f.label "Search for" %>
    <%= text_field_tag :q, params[:q] %>
    <%= submit_tag "Go", name: nil %>
  </p>
<% end %>

 
<ul>
  <% @search_movies.each do |movie| %>
    <li>
      <h3>
       <%= link_to movie.title, controller: "movies", action: "show", id: movie.id%>
     </h3>
    </li>
  <% end %>
</ul>
